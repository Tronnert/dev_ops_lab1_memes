;
; BIND reverse data file for local 192.168.126.xxx network
;
$TTL    604800
@       IN      SOA     srv1.class.local. root.class.local. (
                              7               ; Serial
                          86400           ; Refresh
                          7200             ; Retry
                          3600000      ; Expire
                          172800 )       ; Negative Cache TTL
;
@       IN      NS      srv1.
10      IN      PTR     srv1.class.local.